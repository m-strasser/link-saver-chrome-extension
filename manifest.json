{
        "manifest_version": 2,

        "name": "Link Saver",
        "description": "Saves the link of the current tab and sends it to a server.",
        "version": "0.1",

        "browser_action": {
                "default_icon": "icon.png",
                "default_popup": "popup.html",
                "default_title": "Save link for later"
        },
        "permissions": [
                "activeTab",
                "https://ajax.googleapis.com/"
        ]
}
